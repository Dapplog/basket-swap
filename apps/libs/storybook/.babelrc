{
  "presets": [
    "@babel/env"
  ],
  "plugins": [
    ["@babel/plugin-proposal-nullish-coalescing-operator", { "loose":  true}],
    ["@babel/plugin-syntax-dynamic-import"],
    ["babel-plugin-styled-components", { "displayName": true, "fileName": false }],
    ["@babel/plugin-transform-modules-commonjs"]
  ]
}
