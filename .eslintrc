{
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:react/recommended", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "plugins": ["@babel"],
  "rules": {
    "strict": 0
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
